{
  "name": "@myntra/checkout",
  "version": "1.0.0",
  "description": "Myntra's checkout app",
  "scripts": {
    "fix": "prettier --write 'src/**/*.+(css|js|jsx)'",
    "precommit": "lint-staged",
    "prepush": "npm run test && lint-staged",
    "clean": "rm -rf public/",
    "gen-shell-dev": "cd node_modules/@myntra/m-shell && npm run gen",
    "build-dev": "npm run clean && webpack",
    "compile-shell": "node src/shell/index.js --debug & (sleep 2 && curl localhost:8983)",
    "build-shell-mobile": "cp node_modules/@myntra/m-shell/dist/checkout-mobile-shell.dot dist/mobile_shell.jst",
    "build-shell-desktop": "cp node_modules/@myntra/m-shell/dist/checkout-desktop-shell.dot dist/desktop_shell.jst",
    "build-shell-mobile-body": "cp node_modules/@myntra/m-shell/dist/checkout-mobile-storybookBody.dot dist/mobile_storybookBody.jst",
    "build-shell-mobile-header": "cp node_modules/@myntra/m-shell/dist/checkout-mobile-storybookHeader.dot dist/mobile_storybookHeader.jst",
    "build-shell-desktop-body": "cp node_modules/@myntra/m-shell/dist/checkout-desktop-storybookBody.dot dist/desktop_storybookBody.jst",
    "build-shell-desktop-header": "cp node_modules/@myntra/m-shell/dist/checkout-desktop-storybookHeader.dot dist/desktop_storybookHeader.jst",
    "build-storybook-shell": "mkdir -p dist && npm run gen-shell-dev && npm run build-shell-mobile-header && npm run build-shell-mobile-body && npm run build-shell-desktop-header && npm run build-shell-desktop-body && npm run compile-shell",
    "build-shell": "rm -rf dist/ && mkdir dist && npm run gen-shell-dev && npm run build-shell-mobile && npm run build-shell-desktop && npm run compile-shell",
    "build-prod": "npm run test && npm run clean && npm run build-shell && NODE_ENV=production webpack --node-env production  && npm run cdn",
    "link-setup": "cd ../m-shell && npm link",
    "link": "npm run link-setup && npm link @myntra/m-shell",
    "prod": "npm run build-prod && NODE_ENV=production NODE_SUBENV=production node index.js",
    "dev": "npm run build-shell && npm run build-dev && NODE_ENV=development NODE_SUBENV=stage nodemon --max-old-space-size=4096 --inspect index.js --watch src/ --ignore src/browser/",
    "startdev": "NODE_ENV=development NODE_SUBENV=stage nodemon --inspect index.js --watch src/server/",
    "webpack": "webpack",
    "cdn": "AZURE_STORAGE_CONTAINER=checkout node ./cdn/pushToAzureBlob.js",
    "uploadImages": "AZURE_STORAGE_CONTAINER=checkout UPLOAD_IMAGE=true node ./cdn/pushToAzureBlob.js",
    "test": "jest --env=jsdom --testPathIgnorePatterns=.storybook",
    "setup": "npm run clean && npm i --registry https://registry.myntra.com && npm install && npm run build-prod",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage --testPathIgnorePatterns=.storybook",
    "test:snapshot": "jest --env=jsdom .storybook/storybook.test.js",
    "updateSnapshot": "jest --env=jsdom .storybook/storybook.test.js --update-snapshot",
    "moduletest:coverage": "jest --env=jsdom $SRC_PATH --coverage --collectCoverageFrom=$SRC_PATH/**/*.js",
    "storybook": "npm run build-storybook-shell && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "report": "npm run build-prod --withReport true"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/myntra/checkout.git"
  },
  "keywords": [
    "Checkout",
    "Cart"
  ],
  "author": "Checkout Team",
  "license": "ISC",
  "homepage": "https://bitbucket.org/myntra/checkout#readme",
  "lint-staged": {
    "*.{js,jsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@myntra/m-agent": "1.2.58",
    "@myntra/m-comp": "1.1.179",
    "@myntra/m-shell": "1.2.239",
    "@myntra/range-util": "^1.0.4",
    "@myntra/myx": "1.0.451",
    "@myntra/vision": "1.0.2",
    "@sentry/react": "^7.21.1",
    "@sentry/webpack-plugin": "^1.15.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "acorn": "^6.1.1",
    "assets-webpack-plugin": "7.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.2",
    "base-64": "^0.1.0",
    "body-parser": "1.18.2",
    "compression": "^1.7.1",
    "cookie": "^0.3.1",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.1",
    "crypto-js": "^4.1.1",
    "css-loader": "0.28.9",
    "dot": "1.1.2",
    "http-proxy-middleware": "^2.0.1",
    "jsuri": "^1.3.1",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^6.1.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "madalytics-web": "1.2.22",
    "mini-css-extract-plugin": "2.0.0",
    "myx-config": "~0.1.440",
    "newrelic": "^8.14.1",
    "node-circuitbreaker": "1.0.10",
    "node-statsd": "^0.1.1",
    "postcss-constants": "^0.2.0",
    "postcss-loader": "^2.1.0",
    "preact": "10.0.4",
    "preload-webpack-plugin": "2.2.0",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-lazy-load": "3.0.13",
    "react-router-dom": "5.1.2",
    "serialize-javascript": "^1.4.0",
    "sha256": "^0.2.0",
    "storybook-addon-mock": "^1.0.0",
    "style-loader": "^0.20.1",
    "style-service-client": "^1.0.6",
    "switch-express": "4.1.2",
    "webp-converter": "^2.3.3",
    "webpack": "5.40.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-middleware": "5.0.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.1.1",
    "workbox-webpack-plugin": "6.4.2"
  },
  "devDependencies": {
    "@arcblock/storybook-addon-user-agent": "^0.2.2",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-storyshots": "^6.3.6",
    "@storybook/addon-storyshots-puppeteer": "^6.3.6",
    "@storybook/builder-webpack5": "^6.3.6",
    "@storybook/manager-webpack5": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "azure-storage": "^2.10.3",
    "babel-jest": "^27.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "glob": "^7.1.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-canvas-mock": "^2.1.1",
    "jest-cli": "^27.0.6",
    "jest-image-snapshot": "^4.5.1",
    "mime": "^2.4.4",
    "nodemon": "^1.17.5",
    "puppeteer": "^10.1.0",
    "sinon": "^5.0.7",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "why-did-you-update": "^1.0.6"
  }
}
