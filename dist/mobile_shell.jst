  var script = document.createElement( "script" );
  script.src = src;
  script.async = true;
  ref.parentNode.insertBefore( script, ref );
  if (cb && typeof(cb) === "function") {
    script.onload = cb;
  }
  if (ecb && typeof(ecb) === "function") {
    script.onerror = ecb;
  }
  return script;
}
