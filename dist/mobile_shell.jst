<script async>
	/**
 * Notify
 */
var Notify = (function() {
  var SHOW_NOTIFY = 'notifyShow';
  var notifyMainDiv = document.createElement('div');
  notifyMainDiv.setAttribute("id", "notifyMainDiv");
  notifyMainDiv.setAttribute("class", "notifyMainContainer");

  var getTextWidth = function(iconDiv, actionDiv) {
    if (!iconDiv && !actionDiv) {
      return "100%";
    } else if (iconDiv && !actionDiv) {
      return "70%";
    } else {
      return "57%";
    }
  };

  var getIconDiv = function() {
    var icon = this.config.icon;
    if(icon) {
        if (typeof(icon) === 'object') {
            var iconName = icon.name;
            return iconName
                ? '<div class = "notifyThumbnail icon-' + iconName + '" style = "' + (icon.style || '') +'" ></div>'
                : '';
        } else if (typeof(icon) === 'string') {
            return '<div class = "notifyThumbnail" ><img src = "' + icon + '" /></div>';
        }
    }
    return '';
  };

  var overrideTextStyle = function(styleOverrides) {
    if(styleOverrides)
      return styleOverrides.notifyTextDiv || '';
  };

  var getTemplate = function() {
    var iconDiv = getIconDiv();
    var actionDiv = this.config.action && this.config.action.name ?
    '<div class="notifyActionDiv">' +
    '<button class="notifyAction notifyAction' + this.type + '">' + this.config.action.name +
    '</button></div>' : '';
    return '<div class="notifyContent notify' + this.type + '" style="">' +
        iconDiv +
        '<div class="notifyText" style = "width:' + (getTextWidth(iconDiv, actionDiv)) + ';text-align:' +
        (iconDiv ? "left;" : "center;") + (overrideTextStyle(this.config.styleOverrides)) + '">'
        + (this.config.message || '') + '</div>' +
        actionDiv +
        '</div>';
  };

  var handleNotification = function() {
    notifyMainDiv.className = 'notifyMainContainer';
    var notifyMainDivClass = 'notify' + this.config.position;
    /* preference to styleOverrides object to decide styling of the notifyMainDiv */
    var styleOverrides = this.config.styleOverrides;
    if (styleOverrides) {
      notifyMainDiv.setAttribute("style", styleOverrides.notifyMainDiv || '');
    } else if (!notifyMainDiv.classList.contains(notifyMainDivClass)) {
      notifyMainDiv.classList.add(notifyMainDivClass);
    }
    notifyMainDiv.innerHTML = getTemplate();
    if (this.config.action) {
      notifyMainDiv.querySelector(".notifyAction").addEventListener('click',this.config.action.handler);
    }
    setTimeout(showNotification, 100);
    document.body.appendChild(notifyMainDiv);
    setTimeout(hideNotification, this.config.delay);
  };

  var showNotification = function() {
    if (notifyMainDiv && !notifyMainDiv.classList.contains(SHOW_NOTIFY)) {
      notifyMainDiv.classList.add(SHOW_NOTIFY);
    }
  };

  var onTransitionEnd = function (e) {
    var cL = this.classList;
    if (cL.contains('hidey')) {
      cL.remove('hidey');
    } else {
      cL.add('hidey');
    }
    this.removeEventListener('transitionend', onTransitionEnd, false);
  };

  var hideNotification = function() {
    if (notifyMainDiv && notifyMainDiv.classList.contains(SHOW_NOTIFY)) {
      notifyMainDiv.classList.remove(SHOW_NOTIFY);
      notifyMainDiv.addEventListener('transitionend', onTransitionEnd, false);
    }
  };

  return function(type, config) {
    this.config = Object.assign({
      message: '',
      delay: 4000,
      position: 'top'
    }, config);
    this.type = type ? type.toLowerCase() : 'info';
    handleNotification();
  };
})();
/* Usage -> SHELL.alert('info', { message: 'Hello, World', action: { name: 'Go to', handler: ()=>{} }}) */
function alert (type, config) {
  Notify(type, config);
}
