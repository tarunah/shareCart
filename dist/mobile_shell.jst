<script async>
  var mobileUtils = (function () {


  function setPageIcon(pageName) {
    var pageIconConfig = [{ selector: '.header .myntra-logo', pageName: ['CONFIRMATION', 'ORDER CONFIRMED', 'PAYMENT FAILED, ORDER PLACED', 'PAYMENT PENDING', 'PAYMENT SUCCESSFUL'] },
    { selector: '.header .close-icon', pageName: ['COUPONS', 'GIFT WRAP', 'SELECT SELLER'] },
    { selector: '.header .back-icon', pageName: ['SHOPPING BAG', 'ADDRESS', 'PAYMENT', 'EDIT ADDRESS', 'ADD NEW ADDRESS', 'SELECT ADDRESS'] }];

    pageIconConfig.forEach(function (config) {
      var icon = document.querySelector(config.selector);
      icon &&
        (config.pageName.indexOf(pageName) !== -1 ? icon.classList.remove('hide') : icon.classList.add('hide'));
    });
  }

  var setActivePage = function (pageName, options) {
    options = options || {};
    var pagesWithStepNumber = {
      'SHOPPING BAG': 1,
      ADDRESS: 2,
      PAYMENT: 3
    };
    document.title = pageName;

    var pageNameElm = document.querySelector('.header .page-name');
    var stepNumberElm = document.querySelector('.header .step-number');
    var hideStepNumber = (window.location && window.location.search && window.location.search.indexOf('cartContext=egiftcard') !== -1) || options.hideStepNumber;
    pageNameElm && (pageNameElm.innerText = pageName);

    if (stepNumberElm) {
      /* hides the stepNumber */
      stepNumberElm.classList.add('hide');

      /* unHides stepnumber for pagesWithStepNumber */
      if (pagesWithStepNumber[pageName] && !hideStepNumber) {
        stepNumberElm.innerText = 'STEP ' + pagesWithStepNumber[pageName] + '/3';
        stepNumberElm.classList.remove('hide');
      }
    }
    setPageIcon(pageName);
  };

  var mobileInsideUrls = [
    '/checkout/cart',
    '/checkout/cart/giftwrap',
    '/checkout/cart/coupons',
    '/checkout/cart/sellers',
    '/checkout/address',
    '/checkout/address/list',
    '/checkout/address/add',
    '/checkout/address/edit',
    '/checkout/payment/bnpl/tnc',
    '/checkout/payment'
  ];

  var redirectionRequiredUrls = [
    '/login'
  ];

  var redirectTo = commonUtils.getRedirectToFunc(mobileInsideUrls, redirectionRequiredUrls);

  return { setActivePage: setActivePage, redirectTo: redirectTo };
})();

  var SHELL = {
    alert: alert,
    setActivePage: mobileUtils.setActivePage,
    redirectTo: mobileUtils.redirectTo
  }
</script>
